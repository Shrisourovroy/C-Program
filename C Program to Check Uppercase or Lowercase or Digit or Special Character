#include<stdio.h>
int main()
{
    char n;
    printf("Enter your key: ");
    scanf("%c",&n);

    if(n>=65 && n<90)
    {
        printf("Upercase letter.");
    }
    else if(n>=97 && n<122)
    {
        printf("Lowercase letter");
    }
    else if(n>=48 && n<=57)
        {
            printf("Digit");
        }
    else 
    printf("Special character.");
    return 0;
}
