#include<stdio.h>
int main()
{
    int n;
    printf("Enter your age: ");
    scanf("%d",&n);
    if(n>=18)
    {
        printf("You are eligible for vote.");
    }
    else if(n<18 && n>0)
    {
        printf("You are not eligible for vote.");
    }
    else
        printf("Enter a valid age.");

    return 0;
}
